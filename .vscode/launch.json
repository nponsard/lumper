  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'lumper'",
      "cargo": {
        "args": [
          "build",
          "--bin=lumper",
          "--package=lumper"
        ],
        "filter": {
          "name": "lumper",
          "kind": "bin"
        }
      },
      "args": [
        "--kernel",
        "/home/sautax/do-virt/linux/arch/x86/boot/compressed/vmlinux.bin",
        "-m",
        "2048"
      ],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'lumper'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=lumper",
          "--package=lumper"
        ],
        "filter": {
          "name": "lumper",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}